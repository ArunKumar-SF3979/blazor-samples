<!DOCTYPE html>
<html lang="en">

<head>
    <style>
        #components-reconnect-modal {
            display: none !important;
        }
    </style>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    @{#if DEBUG || STAGING}
        <link rel="preload" href="_content/Syncfusion.Blazor.Themes/fluent2.css" as="style" onload="this.onload=null;this.rel='stylesheet'" id="theme" />
    @{#else}
        <link rel="preload" href="https://cdn.syncfusion.com/blazor/29.1.33/styles/fluent2.css" as="style" onload="this.onload=null;this.rel='stylesheet'" id="theme"/>
    @{#endif}
    <SampleMetaData @rendermode="@RenderMode.InteractiveServer" />
@{#if NET8_0}
    <link href="Blazor_Server_Demos_NET8.styles.css" rel="stylesheet" />
@{#else}
    <link href="@Assets["Blazor_Server_Demos_NET9.styles.css"]" rel="stylesheet" />
    <ImportMap />
@{#endif} 
</head>

<body>
    <AddHeaderTag @rendermode="@RenderMode.InteractiveServer" />
    <div hidden id="sync-analytics" data-queue="EJ2 - Blazor - Demos"></div>
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-W8WD8WN" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <Routes @rendermode="@(new InteractiveServerRenderMode(true))" />
    <script type="module" src="importResources.js"></script>
    <script src="_framework/blazor.web.js"></script>
    @{#if DEBUG || STAGING}
        <script defer src="_content/Syncfusion.Blazor.Core/scripts/syncfusion-blazor.min.js" type="text/javascript"></script>
    @{#else}
        <script defer src="https://cdn.syncfusion.com/blazor/29.1.33/syncfusion-blazor.min.js" type="text/javascript"></script>
    @{#endif}
    <div class="dynamic-resources"></div>
    <script>
        // Google Tag Manager
        setTimeout(function () {
            (function (w, d, s, l, i) {
                w[l] = w[l] || []; w[l].push({
