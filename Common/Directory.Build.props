<Project>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('ServerCommon_NET5')) == 'true'">
      <BaseOutputPath>bin\server-net5\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net5\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('ServerCommon_NET6')) == 'true'">
      <BaseOutputPath>bin\server-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('WasmCommon_NET5')) == 'true'">
      <BaseOutputPath>bin\wasm-net5\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net5\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('WasmCommon_NET6')) == 'true'">
      <BaseOutputPath>bin\wasm-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
</Project>